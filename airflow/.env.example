# Airflow Environment Configuration
# Copy this file to .env and customize the values

# User ID for Airflow containers (use your current user ID)
AIRFLOW_UID=1000

# Custom admin user credentials (CHANGE THESE!)
# These will be used to create the default admin user during initialization
_AIRFLOW_WWW_USER_USERNAME=admin
_AIRFLOW_WWW_USER_PASSWORD=change_me_secure_password

# S3 Configuration
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin123
S3_ENDPOINT_URL=http://minio-s3:9000
S3_BUCKET_NAME=demo-bucket
S3_REGION=us-east-1
